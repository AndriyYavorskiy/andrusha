{
    "name": "Andrii's Extension",
    "provider": "Andrusha",
    "description": "Sample extension inside Shell with Shell navigation",   
    "version": "1.0.0", 
    "icon": "https://www.andrusha-fond.ru/local/templates/andryusha/assets/images/heart-assets/01-h.png",
    "translations": [
      {
        "lang": "ua",
        "name": "І все буде добре!",
        "description": "Це просто пробне розшірення для SAP "
      }
    ],
    "useShellSDK": false,
    "optimalHeight": "480",
    "outletPositions": [
        
    ],
    "insideShell": true,
    "parameters": [{
      "name": "alowContact",
      "type": "BOOLEAN",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Allow contact"
        },
        {
          "lang": "de",
          "value": "Kontakt zulassen"
        }
      ]
    },
    {
      "name": "apiKey",
      "required": true,
      "type": "TEXT",
      "regexValidation": "^[a-zA-Z-]+$",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "API Key"
        },
        {
          "lang": "de",
          "value": "API-Schlüssel"
        }
      ],
      "placeholderTranslations": [
        {
          "lang": "en",
          "value": "An API key received in the confirmation mail"
        },
        {
          "lang": "de",
          "value": "API-Schlüssel, den Sie in der Bestätigungsmail erhalten haben"
        }
      ]
    },
    {
      "name": "networkName",
      "required": true,
      "type": "TEXT",
      "regexValidation": "^[a-zA-Z-]+$",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Net name"
        },
        {
          "lang": "de",
          "value": "Der Netzwerkname"
        }
      ],
      "placeholderTranslations": [
        {
          "lang": "en",
          "value": "The network name"
        },
        {
          "lang": "de",
          "value": "Der Netzwerkname, den Sie in der Bestätigungsmail erhalten haben"
        }
      ]
    }],
    "shellNavigation": {
        "icon": "activities",
        "sideNavigation": [
          {
            "pathSegment": "home",
            "labelTranslations": [
              {
                "lang": "en",
                "value": "Home"
              }
            ],
            "viewUrl": "/#/",
            "icon": "home",
            "isDefault": true,
            "children": []
          }
       ]
    }
}
